function setup() {
	createCanvas(windowWidth, windowHeight);
	background('white');

}

function draw() {
	noStroke();
	
	for (var tree = 1; tree <6; tree +=1) {
		for (var apple = 1; apple <8; apple +=1) {
			
			var x = apple *100;
			var y = tree*100; 
			
			fill('red');
			ellipse(x, y, 50);
			
			fill('green');
			triangle(x, y-10, x, y-40, x +20, y-40);
		
	}
	}
}
